mounts:
  - name: backup
    src: //10.0.11.241/PlexMedia/Youtube-DL
    path: /youtube-dl
    opts: rw,username=guest,password="",vers=3.0,file_mode=0660,dir_mode=0660
    type: cifs

opts: -N 5 --playlist-reverse --download-archive /youtube-dl/YouTube/downloaded.txt -i -o "/youtube-dl/YouTube/%(uploader)s/%(playlist)s/%(upload_date>%Y)s/%(playlist)s - S%(upload_date>%Y)sE%(playlist_index)s - %(title)s.%(ext)s" -f "bestvideo[ext=mp4]+bestaudio[ext=m4a]" -S vcodec:h264 --merge-output-format mkv --add-metadata --write-thumbnail --write-description --write-auto-subs --sub-langs en,sv,-live_chat --convert-subs srt --batch-file=/opt/channel.list --compat-options playlist-index

